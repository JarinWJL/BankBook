.background {
  width: 100%;
  height: 100%;
  position:fixed; /*位置固定*/
  background-size:100% 100%;
  z-index: -1;/*将背景图位置设置为最底层*/
  opacity:0.5/*半透明*/
}
.ident{
  height: 100%;
  width: 100%;
  
  display: flex;/*弹性布局*/
  flex-direction: column;/*竖直排列*/

  
}
.tushu{
  width: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  margin-top: 30rpx;

}
.tushu image{
  height: 360rpx;
  width: 240rpx;
  margin-bottom: 10rpx;/*底部留空*/
  border-radius: 7px;
}
